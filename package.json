{
    "name": "vscode-json-path",
    "version": "2.2.7",
    "description": "Extract and find data in JSON structures with JSONPath queries",
    "repository": {
        "type": "git",
        "url": "https://github.com/weijunyu/vscode-json-path.git"
    },
    "license": "MIT",
    "author": {
        "name": "Wei Junyu",
        "email": "junyuwei@gmail.com"
    },
    "main": "./out/extension",
    "scripts": {
        "compile": "tsc -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test",
        "vscode:prepublish": "npm run compile",
        "watch": "tsc -watch -p ./"
    },
    "dependencies": {
        "jsonpath": "^1.0.2",
        "lodash": "^4.17.14"
    },
    "devDependencies": {
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.6.4",
        "tslint": "^5.18.0",
        "typescript": "^3.5.3",
        "vscode": "^1.1.35"
    },
    "engines": {
        "vscode": "^1.28.0"
    },
    "activationEvents": [
        "onCommand:extension.jsonPath",
        "onCommand:extension.jsonPathWithNodes",
        "onCommand:extension.startTask"
    ],
    "categories": [
        "Other"
    ],
    "contributes": {
        "commands": [
            {
                "command": "extension.jsonPath",
                "title": "jsonpath: Extract JSON data"
            },
            {
                "command": "extension.jsonPathWithNodes",
                "title": "jsonpath: Extract JSON data with their paths"
            },
            {
                "command": "extension.startTask",
                "title": "Start task that shows progress"
            }
        ]
    },
    "displayName": "JSON Path",
    "icon": "icon.png",
    "publisher": "weijunyu"
}
