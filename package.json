{
    "name": "vscode-json-path",
    "displayName": "JSON Path",
    "description": "Given a JSON path, extracts data out of JSON structures",
    "version": "1.0.0",
    "publisher": "placeholder",
    "engines": {
        "vscode": "^1.26.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.jsonPath"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.jsonPath",
                "title": "Get JSON at specified path"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "lodash": "^4.17.10",
        "jsonpath": "^1.0.0"
    },
    "devDependencies": {
        "typescript": "^3.0.0",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
